{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Jute",
  "mainBinaryName": "Jute",
  "version": "0.0.0",
  "identifier": "com.ekzhang.jute",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    },
    "windows": [
      {
        "width": 800,
        "height": 800,
        "fullscreen": false,
        "resizable": true,
        "title": "Jute",
        "titleBarStyle": "Overlay",
        "hiddenTitle": true
      }
    ]
  }
}